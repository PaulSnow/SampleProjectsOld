						</setup>
						<initialActions></initialActions>
						<Condition n='1'>
							<Formal>ThisClient == client</Formal>
							<Postfix>0 local@  client req</Postfix>
							<execute>
								<local_fetch index='0' value='client'></local_fetch>
							</execute>
							<result v='N'></result>
						</Condition>
						<Condition n='2'>
							<Formal>the client is the parent of ThisClient</Formal>
							<Postfix>parent { pop true } { pop false } { dup type streq source client  req and target 0 local@   req and }  relationships forfirstelse</Postfix>
							<execute></execute>
