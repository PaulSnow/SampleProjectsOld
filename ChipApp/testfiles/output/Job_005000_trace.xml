				<decisiontable name='Calculate_Individual_Income'>
					<context execute='{ { /Calculate_Individual_Income executeTable  } dup incomes forall pop } dup clients forall pop'>
						<formal>for all clients</formal>
						<formal>for all incomes</formal>
						<setup></setup>
						<initialActions></initialActions>
						<Condition n='1'>
							<Formal>income.earned == true</Formal>
							<Postfix>income.earned true beq</Postfix>
							<execute></execute>
							<result v='Y'></result>
						</Condition>
						<column n='1 '>
							<action n='1'>
								<formal>add income.amount to the client.totalIncome</formal>
								<def entity='/client 100013 createEntity ' name='client.totalIncome'>333</def>
							</action>
