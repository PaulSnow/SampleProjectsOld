			<action n='3'>
				<formal>Perform Evaluate_CHIP_Eligibility</formal>
				<decisiontable name='Evaluate_CHIP_Eligibility'>
					<context execute='{ { { /Evaluate_CHIP_Eligibility executeTable  } 0 local@  2 *  cvi allocate execute deallocate pop } FPL_Base FPL_PerAdditionalPerson client.IncomeGroupCount 1 -  * +  cvi allocate execute deallocate pop } { dup applying true beq if } clients forall pop'>
						<formal>for all clients whose applying == true</formal>
						<formal>local int FPL = FPL_Base + FPL_PerAdditionalPerson * (client.IncomeGroupCount - 1 )</formal>
						<formal>local int FPL200 = FPL * 2</formal>
						<setup>
							<allocate value='867'></allocate>
							<local_fetch index='0' value='867'></local_fetch>
							<allocate value='1734'></allocate>
						</setup>
